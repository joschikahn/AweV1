@model AweV1.Models.Thesis

@{
    ViewData["Title"] = "Details";
}

<style>
    .panel-default > .panel-heading {
        background-color: #BFD2FA;
    }
</style>

<h2>Thesis: Details</h2>

<div class="panel panel-default">
    <div class="panel-heading">
        <div class="row">
            <div class="allgemeinclass">
                <div class="col-xs-6">
                    <h4>Allgemein</h4>
                </div>
                <div class="col-xs-6">

                </div>
            </div>
        </div>
    </div>

    <table class="table table-bordered">
        <tr>
            <th class="text-left" style="width: 50%">
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th class="text-left" style="width: 50%">
                @Html.DisplayFor(model => model.Title)
            </th>
        </tr>
        <tr>
            <th class="text-left" style="width: 50%">
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th class="text-left" style="width: 50%">
                @Html.DisplayFor(model => model.Description)
            </th>
        </tr>
        <tr>
            <th class="text-left" style="width: 50%">
                @Html.DisplayNameFor(model => model.Summary)
            </th>
            <th class="text-left" style="width: 50%">
                @Html.DisplayFor(model => model.Summary)
            </th>
        </tr>
        <tr>
            <th class="text-left" style="width: 50%">
                @Html.DisplayNameFor(model => model.Supervisor)
            </th>
            <th class="text-left" style="width: 50%">
                @foreach (var supervisor in ViewBag.supervisor)
                {
                    if (supervisor.Id == Model.SupervisorId)
                    {
                        @supervisor.LastName;
                    }
                }
            </th>
        </tr>
        <tr>
            <th class="text-left" style="width: 50%">
                @Html.DisplayNameFor(model => model.Bachelor)
            </th>
            <th class="text-left" style="width: 50%">
                @Html.DisplayFor(model => model.Bachelor)
</tr>
        <tr>
            <th class="text-left" style="width: 50%">
                @Html.DisplayNameFor(model => model.Master)
            </th>
            <th class="text-left" style="width: 50%">
                @Html.DisplayFor(model => model.Master)
            </th>
        </tr>
        <tr>
            <th class="text-left" style="width: 50%">
                @Html.DisplayNameFor(model => model.LastModified)
            </th>
            <th class="text-left" style="width: 50%">
                @Html.DisplayFor(model => model.LastModified)
            </th>
        </tr>
    </table>
</div>


